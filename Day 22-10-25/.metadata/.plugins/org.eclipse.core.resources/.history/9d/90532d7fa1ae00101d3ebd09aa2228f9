package com.demo.services;

public class PatternServicesImpl implements PatternServices {

	@Override
	public void patternTriangle(int n) {
		for (int i = 1; i <= n; i++) {
			for (int j = 1; j <= i; j++) {
				System.out.print(j + " ");
			}
			System.out.println();
		}

	}

	@Override
	public void patternPyramid(int n) {
		// TODO Auto-generated method stub

	}

}
