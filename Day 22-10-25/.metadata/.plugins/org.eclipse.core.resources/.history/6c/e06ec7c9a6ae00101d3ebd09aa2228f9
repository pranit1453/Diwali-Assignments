package com.demo.services;

public class MissingNumberServicesImpl implements MissingNumberServices{

	@Override
	public int missingNumber(int n) {
		int sum=0;
		for(int i=0;i<n;i++) {
			arr[i]=sc.nextInt();
			sum+=arr[i];
		}
		int expectedSum=(n*(n+1))/2;
		int missing = expectedSum-sum;
		return missing;
	}

}
